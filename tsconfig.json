{
  "compilerOptions": {
    "outDir": "dist",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "strict": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@theme/*": [
        "docs/.vitepress/theme/*"
      ]
    },
    "types": [
      "vite/client"
    ]
  },
  "include": [
    "docs/**/*",
    ".vitepress/**/*",
    "*.d.ts"
  ]
}
